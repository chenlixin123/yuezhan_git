.box{
  width: 100%;
  height: 100vh;
  background: white;
}
.search{
  width: 100%;
  margin-top: 10rpx;
  display: flex;
  justify-items: center;
}
.address{
  margin-left: 30rpx;
  margin-right: 30rpx;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  /* border: 1px solid ; */
}
.input_search{
  width: 73%;
  border: 1px solid #C4C4C4;
  padding: 5rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  border-radius: 25rpx;
}
.carousel{
  width: 100%;
  margin-top: 10rpx;
}
.icons{
  width: 100%;
  height: 150rpx;
  margin-top: 20rpx;
  /* border: 1px solid; */
  display: flex;
  justify-content:space-around;
}
.icon{
  width: 50%;
  /* border: 1px solid; */
  height: 100%;
  text-align: center;
}
.icon_img{
  width: 55%;
  height: 100rpx;
  /* border: 1px solid; */
}
.icon_text{
  height: 40rpx;
  font-size: 28rpx;
  margin-top: -15rpx;
  /* border: 1px solid red; */
}
.news{
  width: 99%;
  border-radius: 25rpx;
  border: 1px solid #c4c4c4;
  font-size: 26rpx;
  display: flex;
  justify-content: space-between;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  margin: 0 auto;
  margin-bottom: 15rpx;
}
.new{
  margin-left: 25rpx;
}
.new_but{
  margin-right: 25rpx;
}
.content{
  width: 100%;
  height: 200rpx;
  margin-top: 3rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 23rpx;
  font-weight: 400;
  background: #C069FA;
  position: relative;
}
.content_img{
  width: 150rpx;
  height: 160rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}
.content_title{
  font-weight: bold;
  font-size: 25rpx;
}
.winning{
  margin-left: 20rpx;
  margin-right: 10rpx;
}
.details{
  display: flex;
}
.content_btn{
  width: 150rpx;
  height: 60rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 15rpx;
  background: #34BEC3;
  position: absolute;
  right: 60rpx;
  bottom: 30rpx;
}
.add{
  width:80rpx;
  height: 80rpx;
  font-size: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border: 2rpx solid #C069FA;
  color: #C069FA;
  border-radius: 50rpx;
  position: fixed;
  bottom: -30rpx;
  z-index: 1000;
}